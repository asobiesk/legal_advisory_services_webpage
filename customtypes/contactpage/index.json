{
    "id": "contactpage",
    "label": "ContactPage",
    "format": "page",
    "repeatable": false,
    "status": true,
    "json": {
        "Main": {
            "title": { "type": "Text", "config": { "label": "title", "placeholder": "" } },
            "pitch": { "type": "Text", "config": { "label": "pitch", "placeholder": "" } },
            "phoneheader": { "type": "Text", "config": { "label": "phoneHeader", "placeholder": "" } },
            "phone": { "type": "Text", "config": { "label": "phone", "placeholder": "" } },
            "email": { "type": "Text", "config": { "label": "email", "placeholder": "" } },
            "addressheader": { "type": "Text", "config": { "label": "addressHeader", "placeholder": "" } },
            "address": {
                "type": "StructuredText",
                "config": {
                    "label": "address",
                    "placeholder": "",
                    "allowTargetBlank": true,
                    "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
                }
            },
            "openinghours": { "type": "Text", "config": { "label": "openingHours", "placeholder": "" } },
            "addressgeopoint": { "type": "GeoPoint", "config": { "label": "addressGeoPoint" } },
            "slices": { "type": "Slices", "fieldset": "Slice Zone", "config": { "choices": {} } }
        },
        "SEO & Metadata": {
            "meta_description": {
                "config": { "label": "Meta Description", "placeholder": "A brief summary of the page" },
                "type": "StructuredText"
            },
            "meta_image": {
                "config": { "constraint": { "height": 1260, "width": 2400 }, "label": "Meta Image", "thumbnails": [] },
                "type": "Image"
            },
            "meta_title": {
                "config": {
                    "label": "Meta Title",
                    "placeholder": "A title of the page used for social media and search engines"
                },
                "type": "Text"
            }
        }
    }
}
